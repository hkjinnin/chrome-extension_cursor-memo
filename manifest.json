
{
    "manifest_version": 3,
    "name": "カーソルメモ",
    "version": "0.1",
    "description": "カーソルで選択したものをメモします。",
    "icons": {
        "48": "./appicon75.png"
    },
    "background": {
        "service_worker": "./background.js"
    },
    "action": {
        "default_icon": {
            "32": "appicon75.png"
        },
        "default_popup": "app.html"
    },
    "permissions": ["storage","contextMenus"]
}
